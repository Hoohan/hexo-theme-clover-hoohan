<footer id="footer" class="f-cf">
  <a href="mailto:<%= theme.mail %>"><%= theme.mail %></a>
  <% if (theme.social) { %>
    <% _.forEach(theme.social, function(value, key) { %>
      <% if (value) { %>
        · <a href="<%= url_for(value) %>" target="_blank" class="nav-icn"><%= key %></a>
      <% } %>
    <% }); %>
  <% } %>
  · <a href="<%= theme.ICPurl %>" target="_blank" class="nav-icn"><%= theme.ICP %></a>
  · <a href="<%= theme.govburl %>" target="_blank" class="nav-icn"><%= theme.govb %></a>
  <span class="copyright"><%= theme.copyright %></span>
</footer>